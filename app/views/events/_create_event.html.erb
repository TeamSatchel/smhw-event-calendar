<%= form_with model: event do |form| %>
 <%= form.text_field :title %>
 <%= form.text_field :description %>
 <%= form.text_field :start_date, class: 'datepicker', data: { date_format:"dd-mm-yyyy" }, autocomplete: 'off' %>
 <%= form.text_field :end_date, class: 'datepicker', data: { date_format:"dd-mm-yyyy" }, autocomplete: 'off' %>
 <%= form.submit %>
<% end %>

<script type="text/javascript">
  $(document).ready(function(){
    $('.datepicker').datepicker();
  });
</script>
